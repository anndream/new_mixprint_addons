<?xml version="1.0"?>
<openerp>
	<data>

		<record model="ir.ui.view" id="ineco_users_workcenter_form">
			<field name="name">ineco.users.workcenter.form</field>
			<field name="model">res.users</field>
			<field name="inherit_id" ref="base.view_users_form" />
			<field name="arch" type="xml">

                <xpath expr="//page[@string='Access Rights']" position="after">
                    <page string="Workcenter">
                        <group>
                            <field name="workcenter_ids"/>
                        </group>
                    </page>
                </xpath>
				
			
			</field>
		</record>
	</data>
</openerp>